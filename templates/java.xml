<templateSet group="java">
  <template name="info" value="log.info(&quot;$MESSAGE$: {}&quot;, $VAR_NAME$);" description="" toReformat="false" toShortenFQNames="true">
    <variable name="VAR_NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="MESSAGE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="debug" value="log.debug(&quot;$VAR_NAME_COPY$'s value : {}&quot;, $VAR_NAME$);" description="" toReformat="false" toShortenFQNames="true">
    <variable name="VAR_NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VAR_NAME_COPY" expression="" defaultValue="VAR_NAME" alwaysStopAt="false" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="error" value="log.error(&quot;$VAR_NAME_COPY$'s value : {}&quot;, $VAR_NAME$);" description="" toReformat="false" toShortenFQNames="true">
    <variable name="VAR_NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VAR_NAME_COPY" expression="" defaultValue="VAR_NAME" alwaysStopAt="false" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="step" value="/*&#10;  step $NUMBER$ : $DESCRIPTION$&#10; */" description="" toReformat="false" toShortenFQNames="true">
    <variable name="NUMBER" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="DESCRIPTION" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="todo" value="// TODO: $todo$ Created by $USER$ on  $DATE$ $TIME$." description="adds // TODO" toReformat="true" toShortenFQNames="true">
    <variable name="todo" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="USER" expression="user()" defaultValue="" alwaysStopAt="false" />
    <variable name="DATE" expression="date()" defaultValue="" alwaysStopAt="false" />
    <variable name="TIME" expression="time()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>